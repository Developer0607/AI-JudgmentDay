			<mp-control>false</mp-control>
			<ai-control>true</ai-control>
		</entry>

		<parameters>
			<turn-radius-ft>6000</turn-radius-ft>
			<fixed-turn-radius-ft>500</fixed-turn-radius-ft>
			<rudder-constant>0.75</rudder-constant>
			<speed-constant>1</speed-constant>
		</parameters>

		


	</scenario>
</PropertyList>
